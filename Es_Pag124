package contanani;

public class ContaNani extends Thread {

    public ContaNani(){
        super();
    }
    
    public void run(){
        setName("I settenani");
        System.out.println(Thread.currentThread().getName());
        String[] Nani = {"\nBrontolo", "Pisolo", "Mammolo", "Gongolo", "Eolo", "Cucciolo", "Dotto"};
        for(int i=0;i<7;i++){
            Thread.currentThread().setName(Nani[i]);
            System.out.println(Thread.currentThread().getName()+" ");
        }
    }
    
    public static void main(String[] args) {
       ContaNani thr1 = new ContaNani();
       thr1.start();
    }
    
}
